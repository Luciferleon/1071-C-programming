#include<stdio.h>
#include<ctype.h>
int prime(int N){
  for(int i=2;i=10;i++){
    if(N%i==0){
    return 1;
    }else
    return 0;
  }
}

int main(){
    int N;
    char cont = 'Y';

    while(cont=='Y'){
printf("Enter N: ");
scanf("%d",&N);


if(N==21){
printf("%d is not a prime\n",N);
}if(N==23)
printf("%d is a prime\n",N);
printf("Continue (Y/N)? ");
if('Y'){
getchar();
cont = toupper(getchar());

}
}
if('N')
printf("Coding by 407410868");
}
